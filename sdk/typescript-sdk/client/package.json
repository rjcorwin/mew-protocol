{
  "name": "@mew-protocol/client",
  "version": "0.4.0",
  "description": "MEW protocol client - Multi-Entity Workspace Protocol client SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist build-temp"
  },
  "keywords": [
    "mew",
    "mcp",
    "protocol",
    "websocket",
    "client",
    "multi-entity",
    "workspace"
  ],
  "author": "MEW Working Group",
  "license": "MIT",
  "dependencies": {
    "@mew-protocol/types": "^0.4.0",
    "eventemitter3": "^5.0.1",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^20.14.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rjcorwin/mew-protocol.git",
    "directory": "sdk/typescript-sdk/client"
  },
  "homepage": "https://github.com/rjcorwin/mew-protocol#readme",
  "bugs": {
    "url": "https://github.com/rjcorwin/mew-protocol/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
