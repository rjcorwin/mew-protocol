{
  "name": "@mew-protocol/types",
  "version": "0.2.0",
  "description": "TypeScript type definitions for MEW (Multi-Entity Workspace Protocol)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "node ../../../scripts/build-ts-package.mjs",
    "dev": "tsc --watch --project tsconfig.json",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist build-temp"
  },
  "keywords": [
    "mew",
    "types",
    "typescript",
    "protocol",
    "multi-entity",
    "workspace"
  ],
  "author": "MEW Working Group",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rjcorwin/mew-protocol.git",
    "directory": "sdk/typescript-sdk/types"
  },
  "homepage": "https://github.com/rjcorwin/mew-protocol#readme",
  "bugs": {
    "url": "https://github.com/rjcorwin/mew-protocol/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}