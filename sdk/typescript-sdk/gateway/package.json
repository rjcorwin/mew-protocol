{
  "name": "@mew-protocol/gateway",
  "version": "0.2.0",
  "description": "MEW gateway - Reference implementation of the Multi-Entity Workspace Protocol gateway",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "start:dev": "tsx src/server.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mew",
    "gateway",
    "websocket",
    "multi-entity",
    "workspace",
    "protocol"
  ],
  "author": "MEW Working Group",
  "license": "MIT",
  "dependencies": {
    "@mew-protocol/types": "^0.2.0",
    "@mew-protocol/capability-matcher": "^0.2.0",
    "ws": "^8.18.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "uuid": "^10.0.0",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/ws": "^8.5.10",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^10.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@vitest/ui": "^2.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0",
    "@vitest/coverage-v8": "^2.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rjcorwin/mew-protocol.git",
    "directory": "sdk/typescript-sdk/gateway"
  },
  "homepage": "https://github.com/rjcorwin/mew-protocol#readme",
  "bugs": {
    "url": "https://github.com/rjcorwin/mew-protocol/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}