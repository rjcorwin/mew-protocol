{
  "name": "@mew-protocol/agent",
  "version": "0.4.5",
  "description": "TypeScript agent for MEW Protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mew-agent": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "node -e \"const fs=require('fs');const path=require('path');const target=path.join(__dirname,'dist','index.js');if(fs.existsSync(target)){fs.chmodSync(target,0o755);}\"",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist && rm -f *.tsbuildinfo",
    "prepublishOnly": "npm run clean && npm run build && ls -la dist/"
  },
  "dependencies": {
    "@mew-protocol/client": "^0.4.0",
    "@mew-protocol/participant": "^0.4.0",
    "@mew-protocol/types": "^0.4.0",
    "@types/uuid": "^9.0.8",
    "js-yaml": "^4.1.0",
    "openai": "^5.20.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "mew",
    "protocol",
    "agent",
    "ai",
    "typescript"
  ],
  "author": "MEW Working Group",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "config.example.yaml"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rjcorwin/mew-protocol.git",
    "directory": "sdk/typescript-sdk/agent"
  },
  "homepage": "https://github.com/rjcorwin/mew-protocol#readme",
  "bugs": {
    "url": "https://github.com/rjcorwin/mew-protocol/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
