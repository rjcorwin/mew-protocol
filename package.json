{
  "name": "@mew-protocol/mew",
  "version": "0.5.1",
  "description": "MEW Protocol - Multi-Entity Workspace Protocol SDK and CLI",
  "type": "module",
  "bin": {
    "mew": "dist/cli/index.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/types/index.js",
    "./client": "./dist/client/index.js",
    "./participant": "./dist/participant/index.js",
    "./agent": "./dist/agent/index.js",
    "./gateway": "./dist/gateway/index.js",
    "./capability-matcher": "./dist/capability-matcher/index.js",
    "./bridge": "./dist/bridge/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/cli/index.js",
    "build:bin": "tsup",
    "build:all": "npm run build && npm run build:bin",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "./e2e/run-all-tests.sh --no-llm",
    "test:e2e:all": "./e2e/run-all-tests.sh",
    "test:e2e:llm": "./e2e/run-all-tests.sh",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf dist && rm -f *.tsbuildinfo",
    "prepublishOnly": "npm run clean && npm run build:all"
  },
  "keywords": [
    "mew",
    "protocol",
    "multi-agent",
    "ai",
    "coordination",
    "mcp",
    "model-context-protocol",
    "cli",
    "gateway",
    "client",
    "agent",
    "typescript"
  ],
  "author": "MEW Protocol Working Group",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/server-filesystem": "^0.6.2",
    "@types/electron": "^1.4.38",
    "@types/uuid": "^10.0.0",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "debug": "^4.3.4",
    "electron": "^38.2.2",
    "eventemitter3": "^5.0.1",
    "express": "^4.18.2",
    "ink": "^3.2.0",
    "js-yaml": "^4.1.0",
    "jsonpath-plus": "^10.3.0",
    "micromatch": "^4.0.5",
    "openai": "^5.20.1",
    "phaser": "^3.90.0",
    "pm2": "^6.0.10",
    "react": "^18.2.0",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.14.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjcorwin/mew-protocol.git"
  },
  "homepage": "https://github.com/rjcorwin/mew-protocol#readme",
  "bugs": {
    "url": "https://github.com/rjcorwin/mew-protocol/issues"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
