{
  "name": "mew-protocol",
  "version": "0.3.0",
  "description": "Multi-Entity Workspace Protocol - unified workspace and SDK",
  "private": true,
  "workspaces": [
    "packages/mew"
  ],
  "scripts": {
    "test": "./tests/run-all-tests.sh --no-llm",
    "test:all": "./tests/run-all-tests.sh",
    "test:llm": "./tests/run-all-tests.sh",
    "build": "npm run build --workspace @mew-protocol/mew",
    "build:watch": "npm run build:ts --workspace @mew-protocol/mew -- --watch",
    "clean": "npm run clean --workspace @mew-protocol/mew",
    "lint": "npm run lint --workspace @mew-protocol/mew",
    "format": "npm run format --workspace @mew-protocol/mew"
  },
  "keywords": [
    "mew",
    "protocol",
    "multi-agent",
    "ai",
    "coordination",
    "mcp",
    "model-context-protocol"
  ],
  "author": "MEW Protocol Working Group",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjcorwin/mew-protocol.git"
  },
  "homepage": "https://github.com/rjcorwin/mew-protocol#readme",
  "bugs": {
    "url": "https://github.com/rjcorwin/mew-protocol/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
