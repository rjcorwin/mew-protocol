{
  "name": "mew-protocol",
  "version": "0.3.0",
  "description": "Multi-Entity Workspace Protocol - A protocol for transparent multi-agent coordination",
  "private": true,
  "workspaces": [
    "sdk/typescript-sdk/agent",
    "sdk/typescript-sdk/client",
    "sdk/typescript-sdk/participant",
    "sdk/typescript-sdk/types",
    "sdk/typescript-sdk/capability-matcher",
    "sdk/typescript-sdk/gateway",
    "bridge",
    "cli",
    "tests/*/.mew",
    "spaces/*/.mew"
  ],
  "scripts": {
    "test": "./tests/run-all-tests.sh --no-llm",
    "test:all": "./tests/run-all-tests.sh",
    "test:llm": "./tests/run-all-tests.sh",
    "build": "echo 'No build required for root package'",
    "build:sdk": "npm run build:types && npm run build:matcher && npm run build:client && npm run build:participant && npm run build:agent && npm run build:gateway && npm run build:bridge",
    "build:types": "npm run build --workspace=@mew-protocol/types",
    "build:matcher": "npm run build --workspace=@mew-protocol/capability-matcher",
    "build:client": "npm run build --workspace=@mew-protocol/client",
    "build:participant": "npm run build --workspace=@mew-protocol/participant",
    "build:agent": "npm run build --workspace=@mew-protocol/agent",
    "build:gateway": "npm run build --workspace=@mew-protocol/gateway",
    "build:bridge": "npm run build --workspace=@mew-protocol/bridge",
    "lint": "echo 'Lint individual packages in their directories'"
  },
  "keywords": [
    "mew",
    "protocol",
    "multi-agent",
    "ai",
    "coordination",
    "mcp",
    "model-context-protocol"
  ],
  "author": "MEW Protocol Working Group",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjcorwin/mew-protocol.git"
  },
  "homepage": "https://github.com/rjcorwin/mew-protocol#readme",
  "bugs": {
    "url": "https://github.com/rjcorwin/mew-protocol/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "ink": "^3.2.0",
    "ws": "^8.18.0",
    "uuid": "^9.0.1",
    "@types/uuid": "^9.0.8",
    "js-yaml": "^4.1.0",
    "commander": "^11.1.0",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "prettier": "^3.6.2",
    "eslint": "^9.35.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@types/js-yaml": "^4.0.5",
    "@types/ws": "^8.5.5",
    "@types/debug": "^4.1.12"
  }
}
