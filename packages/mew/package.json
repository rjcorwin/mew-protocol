{
  "name": "@mew-protocol/mew",
  "version": "0.5.0",
  "description": "Unified MEW Protocol SDK, CLI, agent, and bridge",
  "private": false,
  "license": "MIT",
  "bin": {
    "mew": "dist/bin/mew.js",
    "mew-agent": "dist/bin/mew-agent.js",
    "mew-bridge": "dist/bin/mew-bridge.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./participant": {
      "types": "./dist/participant/index.d.ts",
      "default": "./dist/participant/index.js"
    },
    "./agent": {
      "types": "./dist/agent/MEWAgent.d.ts",
      "default": "./dist/agent/index.js"
    },
    "./gateway": {
      "types": "./dist/gateway/index.d.ts",
      "default": "./dist/gateway/index.js"
    },
    "./bridge": {
      "types": "./dist/bridge/index.d.ts",
      "default": "./dist/bridge/index.js"
    },
    "./capability-matcher": {
      "types": "./dist/capability-matcher/index.d.ts",
      "default": "./dist/capability-matcher/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "templates"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:types && npm run build:assets",
    "build:types": "tsc -p tsconfig.build.json",
    "build:assets": "node ./scripts/copy-assets.mjs",
    "test": "tsx --test src/**/*.test.ts"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "debug": "^4.3.4",
    "eventemitter3": "^5.0.1",
    "express": "^4.18.2",
    "ink": "^3.2.0",
    "js-yaml": "^4.1.0",
    "jsonpath-plus": "^8.0.0",
    "micromatch": "^4.0.5",
    "openai": "^5.20.1",
    "pm2": "^6.0.10",
    "react": "^18.2.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/ws": "^8.5.10",
    "@types/uuid": "^10.0.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.5.0"
  }
}
